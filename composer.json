{
  "name": "nerd4ever/oidc-server-bundle",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "Nerd4ever\\OidcServerBundle\\": "src/"
    }
  },
  "require": {
    "symfony/framework-bundle": "5.4.*",
    "league/oauth2-server-bundle": "^0.5.0",
    "symfony/event-dispatcher": "5.4.*",
    "lcobucci/jwt": "4.1.5|^4.2"
  },
  "config": {
    "sort-packages": true
  },
  "archive": {
    "exclude": [
      "build/",
      "tests/",
      ".gitattributes",
      ".gitignore",
      ".styleci.yml",
      ".travis.yml",
      "build.xml",
      "phpunit.xml.dist"
    ]
  },
  "extra": {
    "symfony": {
      "config-dir": "Resources/config",
      "bundle-class": "Nerd4ever\\OidcServerBundle\\Nerd4everOidcServerBundle::class"
    }
  }
}
